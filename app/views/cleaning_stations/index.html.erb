<% @cleaning_stations.each do |cleaning_station| %>

  <h2> <%= cleaning_station.name %> </h2>
  <p> <%= cleaning_station.address %> </p>

  <% cleaning_station.fix_orders.each do |fix_order| %>
    <%= fix_order.post.description %>
    <%= cl_image_tag(fix_order.post.photo.key, width: 300) if fix_order.post.photo.attached? %>
  <% end %>
<% end %>
